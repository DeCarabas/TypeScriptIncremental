<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>TypeScript.Incremental</id>
    <version>1.0</version>
    <authors>John Doty</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <description>A drop-in augmentation for the typescript targets file that
    causes the compiler to run incrementally, potentially saving quite a lot
    of time.</description>

    <language>en-US</language>
    <projectUrl>https://github.com/DeCarabas/TypeScriptIncremental</projectUrl>
    <licenseUrl>https://raw.githubusercontent.com/DeCarabas/TypeScriptIncremental/master/LICENSE</licenseUrl>

    <references>
    </references>
  </metadata>

  <files>
    <file src="bin\Debug\TypeScriptIncremental.dll" target="build" />
    <file src="bin\Debug\TypeScriptIncremental.pdb" target="build" />
    <file src="bin\Debug\TypeScript.Incremental.targets" target="build" />
  </files>    
</package>
